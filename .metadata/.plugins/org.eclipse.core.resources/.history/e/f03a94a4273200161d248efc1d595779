import javax.management.RuntimeErrorException;

import digit.parser.TransformNumber;


public class TransformNumberTest {
	public static void test(Class c){
		TransformNumber transform;
		try{
			transform = (TransformNumber)c.newInstance();
		} catch(Exception e){
			throw new RuntimeException(e);
		}
		for(int i=0; i<5; i++){
			float number = (float)(Math.random()*10000000F)/100;
			System.out.println(number + "\t" + transform.Number2String(number));
		}
		
	}
	
}

